{
  "name": "seerr-notifier",
  "main": "worker.js",
  "compatibility_date": "2026-02-22",

  "observability": {
    "logs": {
      "enabled": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": true
    }
  },

  "assets": {
    "directory": "./templates",
    "binding": "ASSETS"
  },

  "vars": {
    "SECRET_TOKEN": "secret",
    "LANG": "language",
    "SEND_IMAGES": "true",
    "CALLMEBOT_KEYS_user": "keys"
  }
}